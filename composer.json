{
    "name": "makinacorpus/soap-generator",
    "description": "SOAP WSDL+XSD to PHP class generator.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name" : "Pierre RINEAU",
            "email" : "pierre.rineau@processus.org"
        },
        {
            "name" : "Makina Corpus",
            "email" : "pierre.rineau@processus.org"
        }
    ],
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "autoload" : {
        "psr-4": {
            "MakinaCorpus\\SoapGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakinaCorpus\\SoapGenerator\\Tests\\": "tests"
        }
    },
    "require": {
        "php" : ">=8.2",
        "symfony/event-dispatcher": "^7.0",
        "nikic/php-parser": "^5.0",
        "symfony/filesystem": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.52"
    }
}
